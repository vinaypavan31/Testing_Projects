package Questions;

import java.util.Scanner;

public class FibinocciNumber {

	public static void main(String[] args) {
		System.out.println("Enter a range of numbers");
		Scanner sc = new Scanner(System.in);
		
		int num=sc.nextInt();
		int sum=0;
		for(int i=0;i<=num;i++) {
			sum=sum+i;
			System.out.print(i+" "+sum+" ");
		}
		

	}

}
